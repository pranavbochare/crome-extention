{
    "name":"my first extention",
    "description":"base level extention",
    "version":"0.0.1",
    "manifest_version":3,
    "permissions":["activeTab","storage","scripting"],

    "action":{
        "default_popup":"popup.html",
        "default_icon":"icon.png"
    },

    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}